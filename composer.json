{
    "name": "dogado/laroute",
    "description": "Access Laravels URL/Route helper functions, from JavaScript.",
    "keywords"  : ["laravel", "routes", "routing", "javascript"],
    "authors": [
        {
            "name": "Aaron Lord",
            "email": "hello@aaronlord.is"
        },
        {
            "name": "dogado GmbH",
            "email": "dev-team@dogado.de"
        }
    ],
    "license" : "MIT",
    "require": {
        "php" : ">=5.6.0",
        "illuminate/support" : "^5.2|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.2|^6.0|^7.0|^8.0",
        "illuminate/config": "^5.2|^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^5.2|^6.0|^7.0|^8.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dogado\\Laroute\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dogado\\Laroute\\LarouteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
