{
    "name": "dogado/laroute",
    "description": "Access Laravels URL/Route helper functions, from JavaScript.",
    "keywords"  : ["laravel", "routes", "routing", "javascript"],
    "authors": [
        {
            "name": "Aaron Lord",
            "email": "hello@aaronlord.is"
        },
        {
            "name": "dogado GmbH",
            "email": "dev-team@dogado.de"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support" : "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/config": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0"
    },
    "require-dev" : {
        "mockery/mockery": "~1.3",
        "orchestra/testbench": "5.x",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Dogado\\Laroute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dogado\\Tests\\Laroute\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dogado\\Laroute\\LarouteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
